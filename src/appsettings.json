{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://*:5000"
      }
    }
  },

  "License": {
    "FilePath": "license.jwt",
    "PublicKeyPath": "Licensing/PublicKey.pem",
    "RenewEndpoint": "http://localhost:5001/license/renew"
  }
}
